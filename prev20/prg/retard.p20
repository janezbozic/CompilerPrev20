    {
      putChar(((i+48):char));
      putChar((10:char));
      putChar(((n+48):char));
      putChar((10:char));
      putChar((10:char));
      
      

      if i==n then {
          none;
      } else {
        perms(n,n, ns);
        #putInteger(i);
      } where { var j:integer };
    } 

